    // loaded fb is out controll, maybe click helper trigger fb loading is better
    window.app = {};
    var checkState = setInterval(function() {
      var jsloaded, fbLoaded;
      // js loaded then show
      if (window.app.loading === 100) {
        jsloaded = true;
        document.getElementById("helper-background").className += " hide";
      }
      // all ready then load fb
      if (/loaded|complete/.test(document.readyState)) {
        var d = document;
        var s = 'script';
        var id = 'facebook-jssdk';
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = '//connect.facebook.net/zh_TW/sdk.js#xfbml=1&version=v2.5&appId=808398882620079';
        fjs.parentNode.insertBefore(js, fjs);

        fbLoaded = true;
      }
      if(jsloaded && fbLoaded)
        clearInterval(checkState);
    }, 10);
  </script>
